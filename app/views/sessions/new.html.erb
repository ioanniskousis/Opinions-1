<div class="center-div">

  <%= form_with( url: sessions_path ) do |f| %>

    <div class="field">
    <%= label_tag :username, "User Name to Log In" %><br />
    <%= f.text_field :username, autofocus: true  %>
    </div>

    <div class="actions">
      <%= f.submit "Login"  %>
    </div>
  <% end %>

  <%= link_to 'Sign Up', new_user_path %>
</div>